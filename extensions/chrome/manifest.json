{
  "manifest_version": 2,
  "name": "又一个山寨的coplay",
  "description": "这一次，没有汉化。做了一点微小的工作，支持了 https，excited!",
  "version": "0.0.5",
  "icons": {
    "128": "coplay.png"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [
    "peer.min.js",
    "coplay.js",
    "coplay.css",
    "run.js"
  ],
  "browser_action": {
    "default_icon": {
      "128": "coplay.png"
    }
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  }
}
